{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/main/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json"
  },
  "dependencies": {
    "@automapper/sequelize": "^8.7.7",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/throttler": "^5.0.1",
    "@types/passport": "^1.0.13",
    "automapper-classes": "^8.7.12",
    "automapper-core": "^8.7.12",
    "automapper-nestjs": "^8.7.12",
    "automapper-sequelize": "^8.7.12",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "loadash": "^1.0.0",
    "mssql": "^10.0.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.32.6",
    "swagger-ui-express": "^5.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@rahino/pcm(|/.*)$": "<rootDir>/apps/pcm/src/$1",
      "^@rahino/eav(|/.*)$": "<rootDir>/apps/eav/src/$1",
      "^@rahino/ecommerce(|/.*)$": "<rootDir>/apps/e-commerce/src/$1",
      "^@rahino/file(|/.*)$": "<rootDir>/libs/file/src/$1",
      "^@rahino/thumbnail(|/.*)$": "<rootDir>/libs/thumbnail/src/$1",
      "^@rahino/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@rahino/http-exception-filter(|/.*)$": "<rootDir>/libs/http-exception-filter/src/$1",
      "^@rahino/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@rahino/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@rahino/permission-checker(|/.*)$": "<rootDir>/libs/permission-checker/src/$1",
      "^@rahino/response(|/.*)$": "<rootDir>/libs/response/src/$1",
      "^@rahino/query-filter(|/.*)$": "<rootDir>/libs/query-filter/src/$1",
      "^@rahino/pay(|/.*)$": "<rootDir>/libs/pay/src/$1"
    }
  }
}
