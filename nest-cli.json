{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger/plugin"
    ]
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "apps/core",
      "entryFile": "index",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.lib.json"
      }
    },
    "pcm": {
      "type": "library",
      "root": "apps/pcm",
      "entryFile": "index",
      "sourceRoot": "apps/pcm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pcm/tsconfig.lib.json"
      }
    },
    "file": {
      "type": "library",
      "root": "libs/file",
      "entryFile": "index",
      "sourceRoot": "libs/file/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file/tsconfig.lib.json"
      }
    },
    "thumbnail": {
      "type": "library",
      "root": "libs/thumbnail",
      "entryFile": "index",
      "sourceRoot": "libs/thumbnail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/thumbnail/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "http-exception-filter": {
      "type": "library",
      "root": "libs/http-exception-filter",
      "entryFile": "index",
      "sourceRoot": "libs/http-exception-filter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/http-exception-filter/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "permission-checker": {
      "type": "library",
      "root": "libs/permission-checker",
      "entryFile": "index",
      "sourceRoot": "libs/permission-checker/src",
      "compilerOptions": {
        "tsConfigPath": "libs/permission-checker/tsconfig.lib.json"
      }
    },
    "response": {
      "type": "library",
      "root": "libs/response",
      "entryFile": "index",
      "sourceRoot": "libs/response/src",
      "compilerOptions": {
        "tsConfigPath": "libs/response/tsconfig.lib.json"
      }
    }
  }
}