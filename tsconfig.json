{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@rahino/core": ["apps/core/src"],
      "@rahino/core/*": ["apps/core/src/*"],
      "@rahino/coreDashboard": ["apps/core-dashboard/src"],
      "@rahino/coreDashboard/*": ["apps/core-dashboard/src/*"],
      "@rahino/discountCoffe": ["apps/discount-coffe/src"],
      "@rahino/discountCoffe/*": ["apps/discount-coffe/src/*"],
      "@rahino/pcm": ["apps/pcm/src"],
      "@rahino/pcm/*": ["apps/pcm/src/*"],
      "@rahino/eav": ["apps/eav/src"],
      "@rahino/eav/*": ["apps/eav/src/*"],
      "@rahino/ecommerce": ["apps/e-commerce/src"],
      "@rahino/ecommerce/*": ["apps/e-commerce/src/*"],
      "@rahino/bpmn": ["apps/bpmn/src"],
      "@rahino/bpmn/*": ["apps/bpmn/src/*"],
      "@rahino/guarantee": ["apps/guarantee/src"],
      "@rahino/guarantee/*": ["apps/guarantee/src/*"],
      "@rahino/file": ["libs/file/src"],
      "@rahino/file/*": ["libs/file/src/*"],
      "@rahino/thumbnail": ["libs/thumbnail/src"],
      "@rahino/thumbnail/*": ["libs/thumbnail/src/*"],
      "@rahino/http-exception-filter": ["libs/http-exception-filter/src"],
      "@rahino/http-exception-filter/*": ["libs/http-exception-filter/src/*"],
      "@rahino/logger": ["libs/logger/src"],
      "@rahino/logger/*": ["libs/logger/src/*"],
      "@rahino/permission-checker": ["libs/permission-checker/src"],
      "@rahino/permission-checker/*": ["libs/permission-checker/src/*"],
      "@rahino/response": ["libs/response/src"],
      "@rahino/response/*": ["libs/response/src/*"],
      "@rahino/query-filter": ["libs/query-filter/src"],
      "@rahino/query-filter/*": ["libs/query-filter/src/*"],
      "@rahino/pay": ["libs/pay/src"],
      "@rahino/pay/*": ["libs/pay/src/*"],
      "@rahino/redis-client": ["libs/redis-client/src"],
      "@rahino/redis-client/*": ["libs/redis-client/src/*"],
      "@rahino/commontools": ["libs/commontools/src"],
      "@rahino/commontools/*": ["libs/commontools/src/*"],
      "@rahino/minio-client": ["libs/minio-client/src"],
      "@rahino/minio-client/*": ["libs/minio-client/src/*"],
      "@rahino/sms": ["libs/sms/src"],
      "@rahino/sms/*": ["libs/sms/src/*"],
      "@rahino/localdatabase": ["libs/localdatabase/src"],
      "@rahino/localdatabase/*": ["libs/localdatabase/src/*"]
    }
  }
}
